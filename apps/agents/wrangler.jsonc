{
	"name": "parallel-agents",
	"main": "src/index.ts",
	"compatibility_date": "2026-02-21",
	"compatibility_flags": ["nodejs_compat", "nodejs_compat_populate_process_env"],
	"observability": {
		"enabled": true,
	},

	// Durable Object bindings for the Agent
	"durable_objects": {
		"bindings": [
			{
				"name": "ParallelAgent",
				"class_name": "ParallelAgent",
			},
		],
	},

	// SQLite storage for agent state persistence
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": ["ParallelAgent"],
		},
	],

	// Workflows binding
	"workflows": [
		{
			"name": "ParallelWorkflow",
			"class_name": "ParallelWorkflow",
			"binding": "PARALLEL_WORKFLOW",
		},
	],

	// AI Gateway binding
	"ai": {
		"binding": "AI",
	},
}

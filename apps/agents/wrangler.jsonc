{
  "name": "parallel-agents",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-21",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env",
  ],
  "observability": {
    "enabled": true,
  },

  "durable_objects": {
    "bindings": [
      {
        "name": "ParallelAgent",
        "class_name": "ParallelAgent",
      },
    ],
  },

  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["ParallelAgent"],
    },
  ],

  "workflows": [
    {
      "name": "ParallelWorkflow",
      "class_name": "ParallelWorkflow",
      "binding": "PARALLEL_WORKFLOW",
    },
    {
      "name": "HypothesisWorkflow",
      "class_name": "HypothesisWorkflow",
      "binding": "HYPOTHESIS_WORKFLOW",
    },
  ],

  "vars": {
    "SYNC_SERVER_URL": "",
    "CONVEX_URL": "",
    "INTERNAL_API_KEY": "",
    "SANDBOX_URL": "",
    "PARALLEL_API_KEY": "",
  },
}
